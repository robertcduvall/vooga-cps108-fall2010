	</Background>
	
	<Music>
		<Sound name ="music1" />
		<Sound name ="music2" />
		<Sound name ="music3" />
	</Music>
	
	<Map name="Map">
		<MapGroup name = "firstMap" />
			<Association char = "G" object = "IndestructibleTile"></Association>
	</Map>
	
	<Rules>
