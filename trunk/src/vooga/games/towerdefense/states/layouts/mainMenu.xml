<?xml version="1.0" encoding="UTF-8" ?>
<Level name="mainMenuLayout">

	<SpriteGroups>
		<SpriteGroup name="buttons">
			<LevelButton name="easy" x="100" y="400">
				<Visual name="easyButton" />
			</LevelButton>
			<LevelButton name="medium" x="300" y="400">
				<Visual name="mediumButton" />
			</LevelButton>
			<LevelButton name="hard" x="500" y="400">
				<Visual name="hardButton" />
			</LevelButton>
		</SpriteGroup>
		<SpriteGroup name="title">
			<Sprite name="title" x="400" y="100">
				<Visual name="mainMenuTitle" />
			</Sprite>
		</SpriteGroup>
	</SpriteGroups>
	
	<Background name="background">
		<Visual name ="mainMenuBackground" />
	</Background>
	
	<Music name="music">
		<Sound name ="mainMenuMusic" />
	</Music>
	
</Level>