<?xml version="1.0" encoding="UTF-8"?>
<Level name="Doodle" xmloverlaypath="src/vooga/games/doodlejump/resources/overlays.xml">

	<SpriteGroups>
		<SpriteGroup name="doodleGroup">
			<Sprite class="vooga.games.doodlejump.DoodleSprite" random="false" x="400" y="300" vx="0" vy="0" quantity="1">
				<Visual name="doodleRight" />
			</Sprite>
		</SpriteGroup>

		<SpriteGroup name="ballGroup">
			<Sprite class="vooga.games.doodlejump.BallSprite" random="false" x="0" y="0" vx="0" vy="0" quantity="0">
				<Visual name="ball" />
			</Sprite>
		</SpriteGroup>

		<SpriteGroup name="platformGroup">
			<Sprite class="vooga.engine.core.BetterSprite" random="false" x="50" y="50" vx="0" vy="0" quantity="1">
				<Visual name="greenPlatform" />
			</Sprite>
		</SpriteGroup>

		<SpriteGroup name="monsterGroup">
			<Sprite class="vooga.games.doodlejump.monsters.FlyingMonster" random="false" x="100" y="100" vx="0" vy="0" quantity="1">
				<Visual name="greenFlyingMonster" />
			</Sprite>
		</SpriteGroup>

	</SpriteGroups>

	<CollisionGroups>
		<CollisionGroup
			subclass="vooga.games.doodlejump.collisions.DoodleToGreenPlatformCollision">
			<SpriteGroup name="doodleGroup"></SpriteGroup>
			<SpriteGroup name="platformGroup"></SpriteGroup>
		</CollisionGroup>

		<CollisionGroup subclass="vooga.games.doodlejump.collisions.BallToMonsterCollision">
			<SpriteGroup name="ballGroup"></SpriteGroup>
			<SpriteGroup name="monsterGroup"></SpriteGroup>
		</CollisionGroup>
	</CollisionGroups>

	<Background>
		<Visual name = "background" />
	</Background>

	<Music>
		<Sound name="jumpSound" />
		<Sound name="springSound" />
	</Music>

	<Map>

	</Map>

</Level>